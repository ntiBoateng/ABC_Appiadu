    <div class="container"
        style="background-color: #ddd3d3;margin-top: 25px;padding:20px; text-align:center; font-family:Verdana, Geneva, Tahoma, sans-serif">
        <form action="/teens" method="post" autocomplete="off" aria-autocomplete="none" style="color: black;"></form>
        <div class="row">
            <div class="col-md-10 offset-md-1">
 <input type="hidden" name="_id" value="{{teen._id}}">

                <h3>TEENS AND CHILDREN REGISTRATION FORM</h3>
            </div>

        </div>
        <br>

        <div class="row">
            <div class="col-md-4">
                <label for="idNum">ID Number</label>
                <input type="text" value="{{teen.idNum}}" id="idNum" class="form-control">
            </div>
            <div class="col-md-4">
                <span></span>
            </div>
            <div class="col-md-4">
                <label for="formNo">Form No.</label>
                <input type="text" value="{{teen.formNo}}" id="formNo" class="form-control">
            </div>
        </div>

        <hr>

        <div class="row">
            <div class="col-md-4">
                <label for="surname">Surname</label>
                <input type="text" id="surname" value="{{teen.surname}}" class="form-control">
            </div>
            <div class="col-md-4">
                <label for="otherName">Other Name(s)</label>
                <input type="text" id="otherName" value="{{teen.otherName}}" class="form-control">
            </div>
        </div>

        <div class="row" style="margin-top: 20px;">
            <div class="col-md-4">
                <label for="date">DOB</label>
                <input type="date" name="date" value="{{teen.dob}}" id="date" class="form-control">
            </div>
        </div>

        <div class="row" style="margin-top: 20px;">
            <div class="col-md-2">
                <label for="age">Age</label>
                <input type="text" value="{{teen.age}}" class="form-control">
            </div>
            <div class="col-md-4">
                <label><i class="fa fa-user-circle-o" aria-hidden="true"></i> Gender</label><br>
                <label for="genderM"> Male</label>
                <input type="radio" name="gender" id="genderM" value="Male"> &nbsp;
                <label for="genderF">Female</label>
                <input type="radio" name="gender" id="genderF" value="Female">
            </div>
            <div class="col-md-3">
                <label for="hometown">Home Town</label>
                <input type="text" class="form-control" value="{{teen.hometown}}" id="hometown">
            </div>
            <div class="col-md-3">
                <label for="region">Region</label><br>
                <select name="region" aria-placeholder="select" id="region" class="form-control">
                    <option aria-placeholder="Select Region" value="">... select region</option>
                    <option value="Ahafo">Ahafo</option>
                    <option value="Ashanti">Ashanti</option>
                    <option value="Central">Central</option>
                    <option value="Northen">Northen</option>
                    <option value="Greater Accra">Greater Accra</option>
                    <option value="Western North">Western North</option>
                    <option value="Upper East">Upper East</option>
                    <option value="Upper West">Upper West</option>
                    <option value="Western">Western</option>
                    <option value="Volta">Volta</option>
                    <option value="North East">North East</option>
                    <option value="Savannah">Savannah</option>
                    <option value="Oti">Oti</option>
                    <option value="Eastern">Eastern</option>
                    <option value="Brong Ahafo">Brong Ahafo</option>
                    <option value="Bono East">Bono East</option>
                </select>
            </div>
        </div>
        <div class="row" style="margin-top: 20px;">
            <div class="col-md-4">
                <label for="residence">Residence</label>
                <input type="text" class="form-control" value="{{teen.residence}}" id="residence">
            </div>
            <div class="col-md-4">
                <label for="houseNo">House No.</label>
                <input type="text" value="{{teen.houseNo}}" class="form-control" id="houseNo">
            </div>
            <div class="col-md-4">
                <label for="school">Occupation/School</label>
                <input type="text" value="{{teen.school}}" class="form-control" id="school">
            </div>
        </div>
        <div class="row " style="margin-top: 20px;">
            <div class="col-md-4">
                <label for="schoolClass">Sunday School Class</label>
                <input type="text" value="{{teen.sundaySchool}}" class="form-control" id="schoolClass">
            </div>
            <div class="col-md-4">
                <label for="mobile">Mobile</label>
                <input type="text" value="{{teen.mobile}}" class="form-control" id="mobile">
            </div>
            <div class="col-md-4">
                <label for="email">Email</label>
                <input type="text" value="{{teen.email}}" class="form-control" id="email">
            </div>
        </div>
        <div class="row" style="margin-top: 20px;">
            <div class="col-md-3">
                <label for="father">Father's Name</label>
                <input type="text" value="{{teen.fatherName}}" class="form-control" id="father">
            </div>
            <div class="col-md-3">
                <label for="fatherChurch">Church</label>
                <input type="text" value="{{teen.fatherChurch}}" class="form-control" id="fatherChurch">
            </div>
            <div class="col-md-3">
                <label for="occupation">Occupation</label>
                <input type="text" value="{{teen.fatherOccupation}}" class="form-control" id="occupation">
            </div>
            <div class="col-md-3">
                <label for="fatherNo">Phone No.</label>
                <input type="text" value="{{teen.fatherNo}}" class="form-control" id="fatherNo">
            </div>
        </div>

        <div class="row" style="margin-top: 20px;">
            <div class="col-md-3">
                <label for="mother">Mother's Name</label>
                <input type="text" value="{{teen.motherName}}" class="form-control" id="mother">
            </div>
            <div class="col-md-3">
                <label for="motherChurch">Church</label>
                <input type="text" value="{{teen.mother}}" class="form-control" id="motherChurch">
            </div>
            <div class="col-md-3">
                <label for="occupation">Occupation</label>
                <input type="text" value="{{teen.motherOccupation}}" class="form-control" id="occupation">
            </div>
            <div class="col-md-3">
                <label for="motherNo">Phone No.</label>
                <input type="text" value="{{teen.motherNo}}" class="form-control" id="motherNo">
            </div>
        </div>

        <div class="row" style="margin-top: 20px;">
            <div class="col-md-3">
                <label><i class="fa fa-user-circle-o" aria-hidden="true"></i> Baptised?</label><br>
                <label for="bapYes"> Yes</label>
                <input type="radio" name="bap" id="bapYes" value="Yes"> &nbsp;
                <label for="bapNo">No</label>
                <input type="radio" name="bap" id="bapNo" value="No">
            </div>
            <div class="col-md-3">
                <label for="by">By</label>
                <input type="text" value="{{teen.baptisedBy}}" class="form-control" id="by">
            </div>
            <div class="col-md-3">
                <label for="church">Church</label>
                <input type="text" value="{{teen.churchBap}}" class="form-control" id="church">
            </div>
            <div class="col-md-3">
                <label for="certNo">Bapt. Cert. No.</label>
                <input type="text" value="{{teen.certNo}}" class="form-control" id="certNo">
            </div>
        </div>

        <div class="row" style="margin-top: 20px;">
            <div class="col-md-4">
                <label for="dateBapt">Date Baptised</label>
                <input type="date" value="{{teen.baptisedDate}}" name="dateBapt" id="dateBapt" class="form-control">
            </div>
            <div class="col-md-4">
                <span></span>
            </div>
            <div class="col-md-2">
                <label for="memberYear">Membership Year</label>
                <input type="text" value="{{teen.memberYear}}" name="dateBapt" id="memberYear" class="form-control">
            </div>
        </div>
        <br>
        <hr>
        <br>

        <div class="row">
            <div class="col-md-4">
                <span></span>
            </div>
            <div class="col-md-4">
                <label for="positionPrev">
                    <h4>Position in Department</h4>
                </label>
                <select name="positionPrev" id="positionPrev" class="form-control">
                    <option value="">.. select one</option>
                    <option value="Worship Team">Worship Team</option>
                    <option value="Class Leader">Class Leader</option>
                    <option value="Secretary">Secretary</option>
                    <option value="Usher">Usher</option>
                    <option value="Singer">Singer</option>
                    <option value="Member">Member</option>
                </select>
            </div>
            <div class="col-md-4">
                <span></span>
            </div>
        </div> <br><br>
        <h4>Leadership Information <i>(Previous)</i></h4>
        <div class="row" style="margin-top: 20px;">
            <div class="col-md-4">
                <label for="depatName">Name of Department/Committee</label>
                <input type="text" value="{{teen.departmentNamePrev}}" id="depatName" class="form-control">
            </div>
            <div class="col-md-1">
                <span></span>
            </div>
            <div class="col-md-2">
                <label for="yearElect">Year Elected</label>
                <input type="text" value="{{teen.yearElectPrev}}" id="yearElect" class="form-control">
            </div>
            <div class="col-md-1">
                <span></span>
            </div>
            <div class="col-md-4">
                <label for="position">Position</label>
                <input type="text" value="{{teen.positionPrev}}" class="form-control" id="position">
            </div>
        </div>

        <br><br>
        <h4>Leadership Information <i>(Current)</i></h4>
        <div class="row" style="margin-top: 20px;">
            <div class="col-md-4">
                <label for="depatName">Name of Department/Committee</label>
                <input type="text" value="{{teen.departmentNameCur}}" id="depatName" class="form-control">
            </div>
            <div class="col-md-1">
                <span></span>
            </div>
            <div class="col-md-2">
                <label for="yearElect">Year Elected</label>
                <input type="text" value="{{teen.yearElectCur}}" id="yearElect" class="form-control">
            </div>
            <div class="col-md-1">
                <span></span>
            </div>
            <div class="col-md-4">
                <label for="position">Position</label>
                <input type="text" value="{{teen.positionCur}}" class="form-control" id="position">
            </div>
        </div>
        <div class="row" style="margin-top: 20px;">
            <div class="col-md-3">
                <a href="/teens/list" class="btn btn-secondary">View All</a>
            </div>
            <div class="col-md-6">
                <span></span>
            </div>
            <div class="col-md-3">
                <button class="btn btn-primary" type="submit">Submit</button>
            </div>
        </div>
        <br><br><br><br><br>
        </form>
